{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot))",
            "dest": "/$1"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/audit",
            "dest": "/audit.html"
        }
    ]
}